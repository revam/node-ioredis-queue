{
  "name": "ioredis-queue",
  "version": "1.0.0",
  "private": true,
  "description": "Simple redis queue system",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "tslint test/**/*.ts && jest",
    "build": "tsc --noEmit -p . && tslint src/**/*.ts && tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revam/node-ioredis-queue.git"
  },
  "keywords": [
    "redis",
    "ioredis",
    "node",
    "typescript",
    "queue"
  ],
  "author": "Mikal Stordal <mikalstordal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revam/node-ioredis-queue/issues"
  },
  "homepage": "https://github.com/revam/node-ioredis-queue#readme",
  "devDependencies": {
    "@types/ioredis": "^3.2.5",
    "@types/jest": "^22.0.0",
    "@types/node": "^8.5.2",
    "del-cli": "^1.1.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "bson-objectid": "^1.2.2",
    "ioredis": "^3.2.2",
    "is-iterable": "^1.1.1",
    "micro-signals": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
